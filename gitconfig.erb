[user]
  name = Dan Sowter
  email = dan@netengine.com.au
[alias]
  co = checkout
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = /Users/dansowter/.gitignore
  editor = subl3 -w
  autocrlf = input
[apply]
  whitespace = nowarn
[format]
  pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[branch]
  autosetuprebase = always
